<section class="orders-index">

  <header class="page-header">
    <h1>Orders</h1>
  </header>
  <% @orders.each do |order| %>
    <% if current_user && current_user.email == order.email %>
      <div class="orders">
        <li><%= link_to order.id, order %></li>
      </div>
    <% end %>
  <% end %>

</section>
